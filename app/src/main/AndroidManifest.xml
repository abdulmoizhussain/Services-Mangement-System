<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	package="com.example.abdul.servicesmanagementsystem">

	<uses-permission android:name="android.permission.INTERNET" />

	<!-- <category  /> -->
	<application
		android:allowBackup="true"
		android:icon="@drawable/user_app"
		android:label="@string/app_name"
		android:logo="@drawable/user_app"
		android:roundIcon="@drawable/user_app"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<meta-data
			android:name="android.support.VERSION"
			tools:node="remove" />

		<activity android:name=".MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name=".Login"
			android:label="Login" />
		<activity
			android:name=".Categories"
			android:label="Services" />
		<activity android:name=".Products" />
		<activity android:name=".Worker" />
		<activity android:name=".UserProfile" />
		<activity android:name=".BaseActivity" />
		<activity android:name=".RequestService" />
		<activity android:name=".AllRequests"></activity>
	</application>

</manifest>